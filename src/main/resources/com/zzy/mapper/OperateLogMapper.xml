<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zzy.mapper.OperateLogMapper">


    <resultMap id="operateLog" type="com.zzy.pojo.OperateLog">
        <id property="id" column="id"/>
        <result column="emp_name" property="operateEmpName"/>
        <result column="operate_emp_id" property="operateEmpId"/>
        <result column="class_name" property="className"/>
        <result column="method_name" property="methodName"/>
        <result column="return_value" property="returnValue"/>
        <result column="method_params" property="methodParams"/>
        <result column="cost_time" property="costTime"/>
    </resultMap>
    <select id="pageSelect" resultMap="operateLog">
        select
        e.name emp_name,
        ol.operate_emp_id,
        ol.operate_time,
        ol.class_name,
        ol.method_name,
        ol.return_value,
        ol.cost_time,
        ol.method_params,
        ol.id
        from operate_log ol
        left join emp e on ol.operate_emp_id = e.id
    </select>
</mapper>